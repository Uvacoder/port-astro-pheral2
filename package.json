{
  "name": "pheralb",
  "author": "@pheralb_",
  "description": "My personal website built with Remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./src/styles/globals.css -o src/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./src/styles/globals.css -o src/styles/tailwind.css"
  },
  "dependencies": {
    "@react-symbols/icons": "1.1.3",
    "@remix-run/node": "1.7.6",
    "@remix-run/react": "1.7.6",
    "@remix-run/vercel": "1.7.6",
    "@vercel/node": "2.6.4",
    "framer-motion": "7.6.12",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "rehype-prism-plus": "1.5.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.6",
    "@remix-run/eslint-config": "1.7.6",
    "@remix-run/serve": "1.7.6",
    "@tailwindcss/typography": "0.5.8",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.28.0",
    "postcss": "8.4.19",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=16"
  }
}
